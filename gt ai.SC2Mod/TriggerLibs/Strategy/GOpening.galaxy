bool GTProtossOpening1 (int player){
// Traditional Opening
	if (!GTFinishIA(player, c_PB_Assimilator, 1 )){
	  	if (GTFinishIA(player, c_PU_Probe, 11) && (!GTFinishIA(player, c_PU_Probe, 12) || GTFinishIA(player, c_PU_Probe, 13)) && !GTFinishA(player, c_PB_Gateway, 1)){
	  		GTChronoBoost(player, 0, c_PB_Nexus ,"TimeWarpProduction");
		}
	    GTrain(player, c_PU_Probe, 9);
	  	GBuild(player, c_PB_Pylon, 1);
	  	GTrainI(player, c_PU_Probe, 11, c_PB_Pylon, 1);
	    GBuildI(player, c_PB_Gateway, 1,  c_PU_Probe, 11);
	    GTrainI(player, c_PU_Probe, 16, c_PB_Gateway, 1);
		if (AIHasRes(player, 300, 0) && !GTFinishIA(player, c_PB_Nexus, 2)){
				GTExpand2(player);
		}

		GBuildI(player, c_PB_Pylon, 2, c_PB_Nexus, 2);
		GTrainI(player, c_PU_Zealot, 1, c_PB_Pylon, 2);
		GBuildI(player, c_PB_CyberneticsCore, 1, c_PB_Pylon, 2);
	    GBuildI(player, c_PB_Assimilator, 1, c_PB_CyberneticsCore, 1);
	  	GTrainA(player, c_PU_Probe, 18, c_PB_Pylon, 2);

	    
	    return true;
	}
	return false;
}

