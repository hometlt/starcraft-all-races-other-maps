			GTrainI(player, c_ZU_Drone, 54, c_ZU_Overlord, 9);
			GTrainI(player, c_ZU_Overlord, 10, c_ZU_Drone, 54);
			GTrainI(player, c_ZU_Drone, 60, c_ZU_Overlord, 10);
			GTBuildTI(player, 2, 1, c_ZB_Extractor, 2, c_onVespeneGas, c_ZU_Drone, 44);
			GBuildI(player, c_ZB_Extractor, 2, c_ZU_Drone, 46);
			GBuildI(player, c_ZB_EvolutionChamber, 1,  c_ZB_Lair_Alias, 1);
			GBuildI(player, c_ZB_RoachWarren, 1, c_ZB_Extractor, 2);

		}

		    GBuildI(player, c_ZB_Lair_Alias, 1,  c_ZB_Extractor, 2);
		    GTrainI(player, c_ZU_Overlord, 5, c_ZU_Drone, 24);
		    GTrainI(player, c_ZU_Drone, 28, c_ZU_Overlord, 5);
			GTrainI(player, c_ZU_Overlord, 6, c_ZU_Drone, 28);
			GTrainI(player, c_ZU_Drone, 32, c_ZU_Overlord, 6);

			GTrainI(player, c_ZU_Overlord, 7, c_ZU_Drone, 32);
			GTrainI(player, c_ZU_Drone, 40, c_ZU_Overlord, 7);
			GTrainI(player, c_ZU_Overlord, 8, c_ZU_Drone, 40);


			GBuildI(player, c_ZB_EvolutionChamber, 1,  c_ZB_Lair_Alias, 1);
			GBuildI(player, c_ZB_RoachWarren, 1, c_ZB_EvolutionChamber, 1);
		    if (GTFinishA(player, c_ZB_Hatchery_Alias, 2)){
		        						AISetMainTown(player, 1);
		        			}
			if (GTFinishIA(player, c_ZB_EvolutionChamber, 1)){
						GTExpand(player);
					}
			if (GTFinishA(player, c_ZB_Hatchery_Alias, 3)){
					    		GTrain(player, c_ZU_Zergling, GCounterFull(player, c_ZU_Zergling));
