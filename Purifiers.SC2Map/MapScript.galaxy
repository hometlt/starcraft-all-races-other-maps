bool gt_DIFFICULTYWAVES_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    // Actions
    if (!runActions) {
        return true;
    }

    if ((DialogControlGetSelectedItem(gv_difficulty, 1) == 1)) {
        TriggerExecute(gt_ZergWavesEasy, true, false);
    }

    if ((DialogControlGetSelectedItem(gv_difficulty, 1) == 2)) {
        TriggerExecute(gt_ZergWavesMedium, true, false);
    }

    if ((DialogControlGetSelectedItem(gv_difficulty, 1) == 3)) {
        TriggerExecute(gt_ZergWavesHard, true, false);
    }

    if ((DialogControlGetSelectedItem(gv_difficulty, 1) == 4)) {
        TriggerExecute(gt_ZergWavesBrutal, true, false);
    }

    return true;
}

