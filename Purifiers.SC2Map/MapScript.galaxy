        PlayerSetDifficulty(1, 1);
        PlayerSetDifficulty(2, 1);
        PlayerSetDifficulty(3, 1);
    }

    if ((DialogControlGetSelectedItem(gv_difficulty, 1) == 2)) {
        PlayerSetDifficulty(1, 2);
        PlayerSetDifficulty(2, 2);
        PlayerSetDifficulty(3, 2);
    }

    if ((DialogControlGetSelectedItem(gv_difficulty, 1) == 3)) {
        PlayerSetDifficulty(1, 3);
        PlayerSetDifficulty(2, 3);
        PlayerSetDifficulty(3, 3);
    }

    if ((DialogControlGetSelectedItem(gv_difficulty, 1) == 4)) {
        PlayerSetDifficulty(1, 4);
        PlayerSetDifficulty(2, 4);
        PlayerSetDifficulty(3, 4);
    }

    return true;
}

